{
    "method": "reg",
    "type": "dense",
    "generator": "bosz",
    "labels": ["T_eff"],
    "weight": "snr",
    "noise": 1.0,
    "mask_snr": 0.1,
    "normalize_input": false,
    "normalize_output": true,
    "batch": 16,
    "levels": 16,
    "units": 2048,
    "act": "relu",
    "loss": "mean_squared_error",
    "patience": 10000,
    "opt": "sgd",
    "lr_sch": "slowdrop",
    "lr": [0.01, 3.0, 0.85],
    "no_batchnorm": true,
    "dropout": 0.1,
    "dropout-input": false,
    "reg_kernel": [0, 1e-6],
    "reg_bias": [0, 1e-6],
    "aug_scale": 0.1,
    "aug_offset": 0.1,
    "in": [
        "${PFSSPEC_DATA}/train/dataset/stellar/bosz/nowave_100k/train",
        "${PFSSPEC_DATA}/train/dataset/stellar/bosz/nowave_100k/valid"
    ],
    "out": "${PFSSPEC_DATA}/train/run/stellar/bosz",
    "checkpoint_period": 500
}